<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.ruide.dao.IImemberdetailDAO">
	<select id="findAll" resultType="Imemberdetail">
		 select  IMID,IMSEX,IMSCHOOL,IMXUEYUAN,IMMAJOR,IMCLASS,IMPHONE,IMQQ,IMWEIXIN,IMPHOTO,IMSUSHE,IMHOME,IMPOSITION,IMBIRTHDAY,IMDATE,IMLOVES,IMRESUME,IMCAUSE,IMAIM,IMBEIZHU from  IMEMBERDETAIL
	</select>
	<insert id="save" parameterType="Imemberdetail">
		 insert into  IMEMBERDETAIL (IMID,IMSEX,IMSCHOOL,IMXUEYUAN,IMMAJOR,IMCLASS,IMPHONE,IMQQ,IMWEIXIN,IMPHOTO,IMSUSHE,IMHOME,IMPOSITION,IMBIRTHDAY,IMDATE,IMLOVES,IMRESUME,IMCAUSE,IMAIM,IMBEIZHU) values (#{IMID},#{IMSEX},#{IMSCHOOL},#{IMXUEYUAN},#{IMMAJOR},#{IMCLASS},#{IMPHONE},#{IMQQ},#{IMWEIXIN},#{IMPHOTO},#{IMSUSHE},#{IMHOME},#{IMPOSITION},#{IMBIRTHDAY},#{IMDATE},#{IMLOVES},#{IMRESUME},#{IMCAUSE},#{IMAIM},#{IMBEIZHU})
	</insert> 
</mapper>